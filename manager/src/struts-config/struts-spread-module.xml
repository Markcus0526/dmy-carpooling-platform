	<!-- 活动推广 -->
	<package name="spread_activity" extends="json-default,authority" namespace="/spread/activity">
		<action name="searchGift" class="com.nmdy.activity.action.ActivityAction" method="searchGift">
				<result type="json">
				<param name="root">
					resultObj
				</param>
			</result>
		</action>
		
		<action name="activity_*" class="com.nmdy.activity.action.ActivityAction"
			method="{1}">
			<result name="main">/WEB-INF/jsp/spread/activity/main.jsp</result>
			<result name="add">/WEB-INF/jsp/spread/activity/add.jsp</result>
			<result name="showGIft">/WEB-INF/jsp/spread/activity/show_gIft.jsp</result>
			<result name="showActivity">/WEB-INF/jsp/spread/activity/show.jsp</result>
			<result name="editActivity">/WEB-INF/jsp/spread/activity/edit.jsp</result>
			<result name="choseGift" type="chain">activity_add</result>
			<result name="insertRule" type="redirectAction">
				<param name="actionName">activity_add</param>
			</result>
			<result name="removeRule" type="redirectAction">
				<param name="actionName">activity_add</param>
			</result>
			<result name="insertActivityerror" type="chain">activity_add</result>
			<result name="delRule" type="chain">activity_editActivity</result>
			<result name="choseGiftForUpdate" type="chain">activity_editActivity</result>
			<result name="insertActivityRule" type="chain">activity_editActivity</result>
			<result name="updateActivityError" type="chain">activity_editActivity</result>
			<result name="updateActivity" type="chain">activity_main</result>
			<result name="insertActivity" type="redirectAction">
				<param name="actionName">activity_main</param>
			</result>
			<result name="stopActivity" type="redirectAction">
				<param name="actionName">activity_main</param>
			</result>
			<result name="search" type="json">
				<param name="root">jsonObj</param>
			</result>
			<result name="listActivityRole" type="json">
				<param name="root">jsonObj</param>
			</result>
		</action>
	</package>

