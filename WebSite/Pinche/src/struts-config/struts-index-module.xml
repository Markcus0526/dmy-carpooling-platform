<struts>

	<package name="index" extends="json-default" namespace="/index">
	
	<action name="main" class="com.nmdy.login.action.LoginAction"
			method="main">
			<result name="main">
				/WEB-INF/jsp/main.jsp
			</result>
		</action>
		
		<!-- 活动 -->
		<action name="activity_*" class="com.nmdy.index.action.ActivityAction" method="{1}">
			<result name="appFx">/WEB-INF/jsp/index/appFx.jsp</result>
			<result name="createFx">/WEB-INF/jsp/index/createFx.jsp</result>
			<result name="showFx">/WEB-INF/jsp/index/showFx.jsp</result>
			<result name="goonFx">/WEB-INF/jsp/index/goonFx.jsp</result>
			<result name="findMyGift">/WEB-INF/jsp/index/find_myGift.jsp</result>
			<result name="friendVisitBadUrl">/WEB-INF/jsp/index/friend_visit_bad_url.jsp</result>
			<result name="friendVisitUrl">/WEB-INF/jsp/index/friend_visit_url.jsp</result>
			<result name="register">/WEB-INF/jsp/index/register.jsp</result>
			<result name="insertOver">/WEB-INF/jsp/index/register_over.jsp</result>
<!-- 			<result name="insertUser">/WEB-INF/jsp/index/register_success.jsp</result> -->
			<result name="guoqi">/WEB-INF/jsp/index/guoqi.jsp</result>
			<result name="loginWeb">/WEB-INF/jsp/index/login.jsp</result>
			<result name="getMsg" type="json"><param name="root">resultObj</param></result>
			<result name="registerUser" type="json"><param name="root">resultObj</param></result>
			<result name="faqi" type="chain">activity_showFx</result>
		</action>
		
		<action name="insertUser" class="com.nmdy.index.action.ActivityAction" method="insertUser">
			
<!-- 			<interceptor-ref name="tokenSession"></interceptor-ref> -->
			
			  <interceptor-ref name="defaultStack" />
             <!--使用token拦截器-->
            <interceptor-ref name="token" />
              <!--拦截到后的输出界面-->
               <result name="invalid.token">/WEB-INF/jsp/errorSubmit.jsp</result>           
            <result name="insertUser">/WEB-INF/jsp/index/register_success.jsp</result>
             
		</action>
	</package>
	
<!-- 工作站 -->
	<package name="workstation" extends="json-default" namespace="/workstation">
		<action name="list" class="com.nmdy.workStation.action.WorkStationAction" method="list">
			<result name="list">/WEB-INF/jsp/workStation/list.jsp</result>
		</action>
		
		<action name="selectAll" class="com.nmdy.workStation.action.WorkStationAction" method="selectAll">
			<result type="json"><param name="root">jsonMap</param></result>
		</action>
		<action name="selectAllUser" class="com.nmdy.workStation.action.WorkStationAction" method="selectAllUser">
			<result type="json"><param name="root">jsonMap</param></result>
		</action>
	</package>
	
	<package name="controlCenter" extends="json-default" namespace="/controlCenter">
		<action name="list" class="com.nmdy.workStation.action.ControlCenterAction" method="list">
			<result name="list">/WEB-INF/jsp/controlCenter/list.jsp</result>
		</action>
		
	<action name="selectAll" class="com.nmdy.workStation.action.ControlCenterAction" method="selectAll">
			<result name="selectAll" type="json"><param name="root">list</param></result>
		</action>
	</package>
	
	<package name="excel" extends="struts-default" namespace="/excel">
		<action name="pointDetailsToExcel" class="util.excel.action.CustomerExcelAction" method="listBillDetailsToExcel">  
              <!-- Result返回结果为InputSream对象，并处理下载文件的类型  contentType 指定下载文件的文件类型 —— application/vnd.ms-excel-->
			<result name="success" type="stream">
                <param name="contentType">application/vnd.ms-excel</param>
               <!--  返回流 excelStream为action中的流变量名称 -->
                <param name="inputName">excelStream</param>
				  <!-- attachment 这个位置的参数挺特殊的，可以设置成下载时，是否出现个下载提示框，或者直接下载之类的，怎么设置忘了，那个小本子找不到了  filename指定生成的文件名字(这种方式合适动态生成文件名，比如做报表时，一般都要说是几月的统计数据之类)为action中变量
				${downloadFileName} 可以处理中文问题 详见Blog http://www.360doc.com/content/09/1122/22/144699_9566346.shtml -->
				 <param name="contentDisposition">attachment;filename="${downloadFileName}"</param>
	           <param name="bufferSize">1024</param>
            </result>
			 <result name="error">/comm/error.jsp</result>         
		</action>
		
